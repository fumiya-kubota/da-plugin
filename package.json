{
  "name": "da-plugin",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:fumiya-kubota/da-plugin.git",
  "author": "fumiya-kubota <fumiya.kubota@glucose.jp>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "deploy": "yarn build:production && gh-pages -d public",
    "build": "webpack",
    "build:production": "NODE_ENV=production webpack",
    "typecheck": "flow",
    "lint": "eslint src",
    "lint:css": "stylelint src/**/*.css",
    "test": "ava",
    "test:ci": "yarn typecheck & yarn lint && yarn lint:css && yarn test:cover",
    "test:cover": "nyc --reporter=lcov --reporter=text yarn test"
  },
  "dependencies": {
    "da-host": "https://github.com/fumiya-kubota/da-host"
  },
  "devDependencies": {
    "@mizchi/babel-preset": "^1.0.1",
    "@mizchi/postcss-config": "^1.0.0",
    "ava": "^0.18.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.2.2",
    "css-loader": "^0.27.3",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-config-mizchi": "^1.0.0",
    "flow-bin": "^0.42.0",
    "gh-pages": "^0.12.0",
    "istanbul": "^0.4.5",
    "license-webpack-plugin": "^0.4.3",
    "mock-css-modules": "^1.0.0",
    "normalize.css": "^5.0.0",
    "nyc": "^10.1.2",
    "postcss": "^5.2.16",
    "postcss-cli": "^3.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "next",
    "style-loader": "^0.14.1",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "@mizchi/babel-preset",
          [
            "env",
            {
              "targets": {
                "Chrome": 57
              },
              "exclude": [
                "transform-regenerator"
              ]
            }
          ]
        ],
        "plugins": [
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "presets": [
          "@mizchi/babel-preset",
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions",
                  "IE 11"
                ]
              }
            }
          ]
        ]
      }
    }
  }
}
